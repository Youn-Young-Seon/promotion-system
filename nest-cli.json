{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "apps/coupon-service/src",
    "compilerOptions": {
        "deleteOutDir": true,
        "webpack": true,
        "tsConfigPath": "apps/coupon-service/tsconfig.app.json"
    },
    "monorepo": true,
    "root": "apps/coupon-service",
    "projects": {
        "coupon-service": {
            "type": "application",
            "root": "apps/coupon-service",
            "entryFile": "main",
            "sourceRoot": "apps/coupon-service/src",
            "compilerOptions": {
                "tsConfigPath": "apps/coupon-service/tsconfig.app.json"
            }
        },
        "point-service": {
            "type": "application",
            "root": "apps/point-service",
            "entryFile": "main",
            "sourceRoot": "apps/point-service/src",
            "compilerOptions": {
                "tsConfigPath": "apps/point-service/tsconfig.app.json"
            }
        },
        "timesale-service": {
            "type": "application",
            "root": "apps/timesale-service",
            "entryFile": "main",
            "sourceRoot": "apps/timesale-service/src",
            "compilerOptions": {
                "tsConfigPath": "apps/timesale-service/tsconfig.app.json"
            }
        },
        "common": {
            "type": "library",
            "root": "libs/common",
            "entryFile": "index",
            "sourceRoot": "libs/common/src",
            "compilerOptions": {
                "tsConfigPath": "libs/common/tsconfig.lib.json"
            }
        }
    }
}